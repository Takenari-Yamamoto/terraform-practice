terraform {
  required_providers {
    google = {
      source  = "hashicorp/google"
      version = "4.51.0"
    }
  }
}

provider "google" {
  credentials = file("/Users/takenariyamamoto/Downloads/golang-project-377611-721f9059e7d8.json")

  project = var.project
  region  = "us-central1"
